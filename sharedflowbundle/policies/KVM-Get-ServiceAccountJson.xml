<KeyValueMapOperations name='KVM-Get-ServiceAccountJson'>
  <Scope>environment</Scope>
  <ExpiryTimeInSecs>120</ExpiryTimeInSecs>
  <MapName ref='kvm-map'>secrets</MapName>
  <Get assignTo='private.credentialsjson'>
    <Key>
      <Parameter ref='kvm-key-for-credentials-json'>sa-credentials-json</Parameter>
    </Key>
  </Get>
</KeyValueMapOperations>
